
public class Main
{
	public static void main(String[] args) {
	for(int i = 1; i <= 10; i++){
	    if(i%2 == 0){
	        System.out.println("O número "+i+" é par. Pares antes: ");
	        for(int j = 1; j< i; j++){
	            if(j%2==0){
	                System.out.println(j+" ");
	            }
	        }
	        
	        System.out.println();
	    }else {
	        System.out.println("O número " +i+" é impar");
	    }
	}
	}
}
